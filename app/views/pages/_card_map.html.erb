<div class="card card-map" style="background-image: linear-gradient(rgba(41,44,60,0.6) 0%, rgba(80,78,100,0.5) 50%), url(<%= cl_image_path_with_default restaurant.photo&.path, height: 100, width: 191, crop: :fill %>);">
  <div class="card-category card-category-map">
    <%= "#{restaurant.fb_overall_star_rating} #{restaurant.star_rating} - #{restaurant.fb_fan_count} fans" if restaurant.fb_overall_star_rating %>
  </div>
  <div class="card-user">
  </div>
  <div class="card-description card-description-map">
    <h3><%= restaurant.name %></h3>
  </div>
  <%= link_to '', 'http://m.me/HelloYvon', class: 'card-link' %>
</div>
